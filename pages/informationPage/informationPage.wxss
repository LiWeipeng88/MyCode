page{
  border:none;
}
.tab-experience-zhanwei {
  height: 100rpx;
}

.tab-experience {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100rpx;
  z-index: 1000;
}

scroll-view {
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  white-space: nowrap;
}

.tab-experience .tab-item {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  position: relative;
}
.style-one {
  display: table;
  width: 100%;
}

.style-one .tab-item {
  width: 1000rpx;
  display: table-cell;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  position: relative;
}

.style-two {
  overflow: hidden;
  white-space: nowrap;
}

.style-two .tab-item {
  min-width: 23%; 
  display: inline-block;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.tab-experience .tab-item.active{
  position: relative;
  color: #ffbb3e;
}
.tab-experience .tab-item text {
  position: absolute;
  left: 50%;
  right: 50%;
  bottom: 0;
  height: 6rpx;
  transition: all 0.3s;
  opacity: 0;
}
.tab-experience .tab-item.active text {
  left:18rpx;
  right: 18rpx;
  opacity: 1;
}
/* 幻灯样式 */
.info-banner{
  width: 100%;
  height: 360rpx;
  margin:15rpx auto;
  padding: 0 15rpx;
  box-sizing: border-box;
}
.info-banner swiper{
  height: 100%;
  border-radius: 10rpx;
  overflow: hidden;
}
.info-banner swiper image{
  height: 100%;
  width: 100%;
  border-radius: 10rpx;
}
.info-banner .wx-swiper-dot{
  height: 14rpx!important;
  width: 14rpx!important;
  border-radius: 14rpx!important;
  margin:0;
}
/* 列表样式一 */

.style_1 .news-item {
  padding: 20rpx 20rpx;
  background-color: #fff;
  margin-top: 16rpx;
}

.style_1 .news-item .title {
  font-size: 32rpx;
  line-height: 1.6;
  display: block;
  margin-bottom: 5rpx;
  font-weight: bold;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.style_1 .news-item .cover {
  width: 100%;
  height: 320rpx;
  display: block;
  margin-bottom: 15rpx;
}

.style_1 .news-item .intro {
  font-size: 28rpx;
  color: #999;
  display: block;
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.style_1 .news-item.border-b:after {
  height: 0;
}

/* 列表样式2 */

.news-list.style_2 {
  padding-top: 16rpx;
}

.style_2 .news-item {
  padding: 0;
  background-color: #fff;
  overflow: hidden;
}

.style_2 .news-item .cover {
  width: 50%;
  height: 210rpx;
}

.style_2 .news-item .news-intro {
  width: 50%;
  height: 200rpx;
  box-sizing: border-box;
  padding: 15rpx 20rpx;
}

.style_2 .news-item:nth-of-type(2n+1) .cover {
  float: left;
}

.style_2 .news-item:nth-of-type(2n+1) .news-intro {
  float: right;
}

.style_2 .news-item:nth-of-type(2n) .cover {
  float: right;
}

.style_2 .news-item:nth-of-type(2n) .news-intro {
  float: left;
}

.style_2 .news-item .news-intro .title {
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align: center;
  margin-bottom: 5rpx;
  line-height: 1.6;
  font-weight: bold;
}
.style_2 .news-item .news-intro .title .charge-tag{
  color: #fff;
  height: 32rpx;
  width: 32rpx;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
  margin-right: 6rpx;
  position: relative;
  top:-4rpx;
}
.style_2 .news-item .news-intro .intro {
  font-size: 28rpx;
  color: #999;
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
}

.style_2 .news-item.border-b:after {
  height: 0;
}

