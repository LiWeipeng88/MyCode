  background-color:#fff;
  border:1px solid #eee;
  color:#666;
  border-radius:4rpx;
}
.pic-good-item .add-sub .btn.gray{
  background-color: #f5f4f9;
  border-color:#f5f4f9; 
  color: #999;
}
.pic-good-item .add-sub .num{
