/* subpages/personalSet/personalSet.wxss */
page{border:none;}
.input-wrap{
  background-color:#fff;
  padding:0 20rpx;
  margin-top:20rpx;
}
.input-wrap .input-item{
  height: 100rpx;
  line-height: 100rpx;
}
.input-wrap .input-item .desc{
  width:140rpx;
  font-size:30rpx;
  color:#333;
  letter-spacing: 1rpx;
}
.input-wrap .input-item .input-box input{
  padding: 0 25rpx;
  color:#111;
}
.input-wrap .input-item .jt-icon{
  width:16rpx;
  height:26.67rpx;
  margin-left:20rpx;
}
.input-wrap .input-item .jt-icon image{
  display: block;
  width:100%;
  height:100%;
}
.btn{
  width:30%;
  height:80rpx;
  line-height:80rpx;
  text-align: center;
  color:#fff;
  margin:50rpx auto 0;
  background-color:#2FCD98;
  font-size:30rpx;
  letter-spacing: 1rpx;
  border-radius:40rpx;
}
