.detail-box .detail-con { font-size: 26rpx; color: #909090; line-height: 1.5; letter-spacing: 2rpx; text-align: justify; text-justify: inter-ideograph; }
.wxParse { padding: 10rpx 0; margin: 0; color: #333; font-size: 26rpx; line-height: 1.6; font-family: helvetica, arial, "Hiragino Sans GB", "Microsoft Yahei", "微软雅黑", sans-serif; }
