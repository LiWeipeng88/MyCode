}
.share-btn button{
  position: absolute; 
  left: 0; 
  top: 0; 
  height: 100%; 
  width: 100%; 
  margin: 0; 
  opacity: 0;  
  z-index: 2;
}


@keyframes memberShow
{
0% {opacity:0.6;}
50% {opacity:1;}
100% {opacity:0;}
}

@-webkit-keyframes memberShow
{
0% {opacity:0.6;}
50% {opacity:1;}
100% {opacity:0;}
}
