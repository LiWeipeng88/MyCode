}
.time-wrap{
  color:#666;
  font-size:34rpx;
  background-color:#f8f8f8;
  width:710rpx;
  text-align:center;
  padding:25rpx 0;
  border-radius:20rpx;
  margin:0 auto;
}
.time-wrap .desc{
  color:#333;
}
.detail-wrap{
  padding:20rpx;
}
.detail-wrap .name{
  color:#111;
  font-size:32rpx;
  letter-spacing: 1rpx;
}
.detail-wrap .brief{
  font-size:26rpx;
  color:#999;
  margin-top:5rpx;
}
.detail-wrap .goods-img{margin-top:15rpx;}
.detail-wrap .goods-img .img-box{display:inline-block;width:223rpx;height:223rpx;border-radius:10rpx;margin-right:20.5rpx;margin-bottom:15rpx;}
.detail-wrap .goods-img .img-box:nth-of-type(3n){margin-right:0;}
.detail-wrap .goods-img .img-box image{display:block;width:100%;height:100%;}

.goods-wrap .title{
  color:#111;
  font-weight: bold;
  font-size:32rpx;
  letter-spacing: 1rpx;
  text-align: center;
  margin:30rpx 0 50rpx;
}
.goods-wrap .title text{
  position:relative;
}
.goods-wrap .title text:after{
  content:'';
  width:50%;
  height:8rpx;
  background-color:#2CC08C; /*FF7800*/
  position:absolute;
  left:25%;
  top:50rpx;
}
.goods-wrap .good-list{
  padding:0 20rpx;
}
.goods-wrap .good-item{
  padding:20rpx;
  box-sizing: border-box;
  border-radius:4rpx;
  box-shadow: 0 0 10rpx #dedede;
  margin-bottom:20rpx;
  position: relative;
}
.goods-wrap .good-item .cover{
  width:180rpx;
  height:180rpx;
  margin-right:20rpx;
  position:relative;
}
.goods-wrap .good-item .cover .zhezhao{background:rgba(0,0,0,0.3);position:absolute;top:0;left:0;right:0;bottom:0;}
.goods-wrap .good-item .cover image{
  display: block;
  width:100%;
  height:100%;
}
.goods-wrap .good-item .name{
  height:60rpx;
  font-size:30rpx;
  line-height:28rpx;
  display: -webkit-box !important; 
  overflow: hidden;
  text-overflow:ellipsis;
  word-break:break-all;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
}
.goods-wrap .good-item .price{
position: absolute;
top: 13px;
right: 0px;
width:180rpx;
height:47rpx;
font-weight: bold ;
/* border:1px solid rgb(49,192,140); */
text-align:center;
border-radius:24rpx;
color:#FF7800;
}

.goods-wrap .good-item .infor-wrap{
  font-size:30rpx;
  color:#333;
  letter-spacing: 1rpx;
}
.goods-wrap .good-item .money-wrap{
  font-size:26rpx;
  color:#999;
  margin-bottom:10rpx;
}
.goods-wrap .good-item .money{
  font-size:32rpx;
  color:#FF7800;
  margin-right:20rpx;
}
.goods-wrap .good-item .money .label{
  font-size:24rpx;
  color:#2CC08C;
  border:1rpx solid #2CC08C;
  padding:5rpx 10rpx;
  border-radius:20rpx;
  position:relative;
  top:-4rpx;
}
.goods-wrap .good-item .shop-money{
  color:#999;
  font-size:24rpx;
}
.goods-wrap .good-item .add-sub .btn{
  display: inline-block;
  width:100rpx;
  height:70rpx;
  line-height:70rpx;
  text-align: center;
  background-color:#F0F0F0;
  color:#666;
  border-radius:4rpx;
}
.goods-wrap .good-item .add-sub .num{
  padding:0 20rpx;
  color:#111;
  font-weight:bold;
}
