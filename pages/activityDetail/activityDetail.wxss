}
.bottom-opreation .btn{
  padding:0 40rpx;
  color:#999;
  text-align: center;
  font-size:24rpx;
  position: relative;
  margin-top:10rpx;
}
.bottom-opreation .btn button{
  position: absolute; 
  left: 0; 
  top: 0; 
  height: 100%; 
  width: 100%; 
  margin: 0; 
  opacity: 0;  
  z-index: 2;
}
.bottom-opreation .btn .icon{
  width:35rpx;
