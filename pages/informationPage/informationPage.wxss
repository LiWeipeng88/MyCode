/* 信息流版列表样式 */
.info-list{
  padding: 0 20rpx;
  background-color: #fff;
}
/* 公用样式 */
.info-item .title{
  font-size: 32rpx;
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow:ellipsis;
  word-break:break-all;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:3;
  line-height: 46rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
}
.title .charge-tag{
  color: #fff;
  height: 32rpx;
  width: 32rpx;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
  margin-right: 6rpx;
  position: relative;
  top:-4rpx;
}
.info-item .other-info .tag-box{
  display: inline-block;
  vertical-align: middle;
  margin-right:15rpx;
}
.other-info .tag-box image{
  display: inline-block;
  vertical-align: middle;
  height: 30rpx;
  width: 30rpx;
  margin-right: 5rpx;
}
.other-info .tag-box text{
  display: inline-block;
  vertical-align: middle;
  font-size: 24rpx;
  color: #999;
  line-height: 30rpx;
  height: 30rpx;
}
.other-info .tag-box .time{
  margin-left: 16rpx;
}
/* 单图样式 */
.info-item .single-img .title{
  min-height: 110rpx;
}
.info-item .single-img{
  align-items: flex-start;
  padding: 20rpx 0;
}
.info-item .single-img .img-ele{
  width: 230rpx;
  height: 150rpx;
}
.info-item .single-img .img-ele image{
  height: 100%;
  width: 100%;
  border:1rpx solid #f8f8f8;
  box-sizing: border-box;
}
/* 三图片样式 */
.three-img{
  padding: 20rpx 0;
}
.three-img .img-list{
  overflow: hidden;
  padding: 10rpx 0;
}
.three-img .img-list .img-ele{
  width: 33.33%;
  float: left;
  box-sizing: border-box;
}
.three-img .img-list .img-ele image{
  display: block;
  width: 230rpx;
  height: 150rpx;
  border:1rpx solid #f8f8f8;
  box-sizing: border-box;
  margin:0 auto;
}
/* 视频样式 */
.video-img{
  padding: 20rpx 0;
}
.video-box{
  position: relative;
  padding: 10rpx 0;
}
.video-box .play-btn{
  position: absolute;
  left:50%;
  top:50%;
  margin-top: -44rpx;
  margin-left: -44rpx;
  padding: 22rpx 18rpx 18rpx 22rpx;
  width: 94rpx;
  height: 94rpx;
  background-color: rgba(0, 0, 0, .4);
  box-sizing: border-box;
  border-radius: 50%;
  z-index: 2;
}
.video-box .play-btn image{
  width: 54rpx;
  height: 54rpx;
  display: block;
  margin:0 auto;
}
.video-box .img-ele{
  width: 100%;
  height: 400rpx;
  display: block;
  border:1rpx solid #f8f8f8;
  box-sizing: border-box;
}
/* 客服样式 */
.contact-wrap{
  left:auto;
  right: 20rpx;
}