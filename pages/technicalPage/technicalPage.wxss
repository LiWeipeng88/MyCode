.infor-box .infor .phone { height: 30rpx; line-height: 30rpx; margin-top: 2%; }
.infor-box .infor-row { height: 80rpx; box-sizing: border-box; }
.infor-box .infor-row .col { width: 50%; height: 50rpx; margin: 15rpx 0; font-size: 30rpx; float: left; text-align: center; }
.infor-box .infor-row .col .col-con { width: 50%; margin: 0 25%; }
