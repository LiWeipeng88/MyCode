.page-tip .loading-box { position: absolute; left: 0; top: 45%; width: 100%; transform: translateY(-50%); }
.page-tip .tip-txt { color: #999; text-align: center; font-size: 28rpx; line-height: 2; }

/**index.wxss**/
.banner-wrap { height: 400rpx; position: relative; }
.banner-wrap swiper { height: 100%; }
.banner-wrap image { height: 100%; width: 100%; }
.wx-swiper-dot { height: 15rpx !important; width: 15rpx !important; border-radius: 15rpx !important; margin: 0; }
.video-play { position: absolute; bottom: 50rpx; left: 50%; transform: translateX(-50%); }
.video-play .play-btn { height: 80rpx; width: 80rpx; border-radius: 50%; padding: 15rpx; box-sizing: border-box; position: absolute; left: 0; top: -14rpx; border: 1px solid rgba(0, 0, 0, 0.1); box-shadow: 0 0 8rpx rgba(0, 0, 0, 0.2); background-color: #fff; }
.video_hover .play-btn { background-color: #f0f0f0; }
.video-play .video-duration { display: block; height: 52rpx; line-height: 52rpx; padding: 0 25rpx; padding-left: 70rpx; border-radius: 26rpx; border: 1px solid rgba(255, 255, 255, 0.5); box-shadow: 0 0 8rpx rgba(0, 0, 0, 0.2); background-color: rgba(0, 0, 0, 0.3); color: #fff; margin-left: 20rpx; }
.video-box { position: fixed; width: 100%; height: 100%; left: 0; top: 0; z-index: 1000000; }
.video-box .video-modal { position: absolute; width: 100%; height: 100%; background-color: #111; z-index: 10; }
.video-box .video-modal .close { position: absolute; top: 30rpx; right: 30rpx; color: #333; background-color: rgba(255, 255, 255, 0.8); border-radius: 50%; height: 60rpx; width: 60rpx; padding: 16rpx; text-align: center; box-sizing: border-box; }
.video-box video { width: 750rpx; height: 420rpx; position: absolute; top: 50%; left: 0; transform: translateY(-50%); z-index: 100; }
