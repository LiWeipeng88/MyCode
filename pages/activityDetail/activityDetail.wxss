}
.bottom-opreation .btn .num{
  width:35rpx;
  height:35rpx;
  text-align: center;
  line-height:35rpx;
  color:#fff;
  font-size:22rpx;
  background-color:#FF7800;
  border-radius:50%;
  position:absolute;
  top:-10rpx;
  right:-15rpx;
}
.bottom-opreation .buy-btn{
  text-align: center;
  height:100rpx;
