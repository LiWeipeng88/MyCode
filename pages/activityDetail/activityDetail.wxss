.peo-infor{
  padding:20rpx;
}
.peo-infor .avatar{
  width:90rpx;
  height:90rpx;
  margin-right:10rpx;
}
.peo-infor .avatar image{
  display: block;
  width:100%;
  height:100%;
  border-radius:50%;
}
.peo-infor .nickname{
  color:#111;
  font-size:30rpx;
}
.peo-infor .time{
  color:#999;
  font-size:26rpx;
}
.peo-infor .phone-icon{
  width:70rpx;
  height:70rpx; 
}
.peo-infor .phone-icon image{
  display: block;
  width:100%;
